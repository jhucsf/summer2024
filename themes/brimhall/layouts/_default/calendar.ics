BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Advanced Research in Cryptography//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
BEGIN:VTIMEZONE
TZID:America/New_York
X-LIC-LOCATION:America/New_York
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
END:STANDARD
END:VTIMEZONE
{{ range .Site.Data.calendar }}
BEGIN:VEVENT
ORGANIZER;CN=Advanced Resarch in Cryptography:mailto:arc@isi.jhu.edu
SUMMARY:{{ .title }}
DESCRIPTION:{{ .title }}
UID:{{ (print .title .date) | sha256 }}@arc.isi.jhu.edu
SEQUENCE:0
STATUS:CONFIRMED
DTSTAMP:{{dateFormat "20060102" .date}}
DTSTART:{{dateFormat "20060102" .date}}
DTEND:{{dateFormat "20060102" .date}}
LOCATION:{{ .link }}
URL:{{ .link }}
END:VEVENT
{{ end }}
END:VCALENDAR